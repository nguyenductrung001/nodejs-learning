<% for(var i = 0; i < users.length; i ++) { %>
<tr>
    <td><%= i +1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user" class="btn border-shodow update">
            <span class="text-grandient"> <i class="fas fa-pencil-alt"></i></span>
        </a>
        <a class="btn border-shodow delete">
            <span class="text-grandient"> <i class="fas fa-times"></i></span>
        </a>
    </td>
</tr>
<% } %>